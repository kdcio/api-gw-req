{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "complexity": ["error", 5],
    "max-lines": ["error", 50],
    "prettier/prettier": ["error"]
  },
  "env": {
    "commonjs": true,
    "node": true,
    "jest": true
  },
  "ignorePatterns": ["node_modules/", "coverage/"]
}
